{
  "name": "clicbeat-nodejs",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts ",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc -p tsconfig.json",
    "prod": "node dist/",
    "create:migrate": "cd src && ../node_modules/.bin/sequelize migration:create --name",
    "migrate": "cd src && ../node_modules/.bin/sequelize db:migrate",
    "migrate:undo": "cd src && ../node_modules/.bin/sequelize db:migrate:undo:all",
    "migrate:undo:one": "cd src && ../node_modules/.bin/sequelize db:migrate:undo --name",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "http://192.168.1.11:8007/niravp/clicbeat-nodejs.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.777.0",
    "@fastify/auth": "^5.0.2",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.1",
    "@fastify/static": "^8.1.1",
    "@fastify/swagger": "^9.4.2",
    "@fastify/swagger-ui": "^5.2.2",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.6.0",
    "dotenv": "^16.4.7",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "fastify": "^5.2.2",
    "fastify-multer": "^2.0.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.6",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "nodemon": "^3.1.9",
    "pino-pretty": "^13.0.0",
    "typescript": "^5"
  }
